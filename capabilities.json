                "outsideFill": {
                    "displayName": "Outside fill",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                }
            }
        },
        "legend": {
            "displayName": "Legend",
            "properties": {
                "show": {
                    "displayName": "Show",
                    "type": {
                        "bool": true
                    }
                },
                "position": {
                    "displayName": "Position",
                    "description": "Legend position",
                    "type": {
                        "enumeration": [{
                            "value": "Top",
