                    "type": {
                        "numeric": true
                    }
                },
                "fontSize": {
                    "displayName": "Text size",
                    "type": {
                        "formatting": {
                            "fontSize": true
                        }
                    }
                },
                "labelDisplayUnits": {
                    "displayName": "Display Units",
