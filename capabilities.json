                        }, {
                            "value": "Bottom",
                            "displayName": "Bottom"
                        }, {
                            "value": "Left",
                            "displayName": "Left"
                        }, {
                            "value": "Right",
                            "displayName": "Right"
                        }, {
                            "value": "TopCenter",
                            "displayName": "Top Center"
                        }, {
                            "value": "BottomCenter",
